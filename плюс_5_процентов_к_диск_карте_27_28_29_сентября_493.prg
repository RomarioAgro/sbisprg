функция плюс_5_процентов_к_диск_карте_27_28_29_сентября_493(пКодарт)
{
пСтрокаМаг=";OK;MD;DM;PR;SL;ES;UZ;PK;CP;RS;SD;AT;UP;KM;TP;BK;UK;BD;PY;OO;BU;OB;OD;UM;PM;PB;SB;GM;CB;CD;";
Если(Найти(пСтрокаМаг,КаСНОМ)==0) Вернуть(0);
  пКолСтрокакции=1;
  пСч=1;
  Пока(пСч<пКолСтрокакции)
  {
    Если(Найти(пСтрока[пСч], пКодарт)!=0)
      {пСч=1000;}#типа выход из цикла}
    пСч++;
  }
  Если(пСч>=1000) Вернуть(1) иначе Вернуть(0);
}
