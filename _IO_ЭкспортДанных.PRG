Функция _IO_ЭкспортДанных()
{
   пТВ1=ТекВремя;
   ПроверитьКонстанту( "ИМП_ЧП", "ЧП для выгрузки",
                               "ЧП Юдинцев С.Л.", "ИМПОРТ", "Импорт" );
   ПроверитьКонстанту( "ИМП_НМГ", "Магазин для выгрузки",
                               "м-н \"\"Европейский\"\"", "ИМПОРТ", "Импорт" );
   Если(ДаНет("Выполняется экспорт данных ?")==0) Вернуть 0;
   пТолькоDBF = ДаНет("Сформировать только DBF-отчет ?",2);
   СпроситьДаты("Укажите период ?");
   оВыборКат=СоздатьДиалог("Выбор каталога");
   оВыборКат.Имя="C:\\3_NABASU\\";
   Если(ВыполнитьДиалог(оВыборКат)==0) Вернуть 0;

   пПуть="d:\\kassa\\out\\";
   оФайл=Объект(пПуть+"nuk.txt"); оФайл=Нет;

   Запустить("cmd /c copy "+пПуть+"nuk.txt "+пПуть+"nuk.000 > null",1);
   Запустить("cmd /c del /S /Q "+пПуть+"*.0?? > null",1);
   Запустить("cmd /c del /S /Q "+пПуть+"*.txt > null",1);

   _IO_ЭкспортД("СВОД");
   Если(пТолькоDBF!=1) _IO_ЭкспортД("ВОЗВ"); 
   Если(пТолькоDBF!=1) _IO_ЭкспортД("БРАК"); 

   пТВ2=ТекВремя;
   Время пТВР;
   пТВР=пТВ2-пТВ1;
   Сообщить(пТВ1+"\n"+пТВ2+"\n"+пТВР);
   Вернуть 1;
}
